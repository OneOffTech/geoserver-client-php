{
    "name": "oneofftech/geoserver-client-php",
    "description": "A GeoServer API client",
    "type": "library",
    "homepage": "https://github.com/oneofftech/geoserver-client-php",
    "license": "AGPL-3.0-only",
    "authors": [
        {
            "name": "Alessio Vertemati",
            "email": "alessio@oneofftech.xyz"
        }
    ],

    "support": {
        "issues": "https://github.com/oneofftech/geoserver-client-php/issues",
        "source": "https://github.com/oneofftech/geoserver-client-php"
    },

    "autoload": {
        "psr-4": {
            "OneOffTech\\GeoServer\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        
        "files": [
            "tests/helpers.php"
        ]
    },
    
    "require": {
        "php": ">=7.1",
        "ext-fileinfo": "*",
        "ext-zip": "*",
        "jms/serializer": "^1.9",
        "php-http/client-implementation": "^1.0",
        "php-http/message": "^1.6",
        "php-http/discovery": "^1.3",
        "doctrine/annotations": "^1.4",
        "doctrine/cache": "^1.6",
        "php-http/client-common": "^1.5",
        "justinrainbow/json-schema": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "php-http/curl-client": "~1.7.0",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "guzzlehttp/psr7": "^1.4",
        "php-http/mock-client": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.12",
        "symfony/var-dumper": "^4.1"
    },
    "extra": {
        "branch-alias" : {
            "dev-master": "0.1.0-dev"
        }
    }
}
